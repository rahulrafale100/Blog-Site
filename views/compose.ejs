<%- include("partial/header") -%>
<h1>Compose</h1>
<form action="/compose" method="post">
  <div class="form-group">
    <label>Title</label>
    <input type="text" class="form-control" name="titleBody" />
  </div>
  <div class="form-group">
    <label>Post</label>
    <textarea name="postBody" class="form-control" id="" rows="5"></textarea>
  </div>
  <button type="submit" class="btn btn-primary">Publish</button>
</form>

<%- include("partial/footer") -%>
